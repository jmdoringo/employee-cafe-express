{
  "name": "employee-cafe-express",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "start": "ts-node --experimental-specifier-resolution=node dist/app.js",
    "stop": "taskkill -f -im node.exe",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q --experimental-specifier-resolution=node dist/app.js\""
  },
  "dependencies": {
    "@types/mysql": "^2.15.21",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.197",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.1",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.1.6"
  }
}
